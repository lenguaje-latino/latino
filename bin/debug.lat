funcion fibo_iter(n)
    si ( n < 2 )
        retorno n
    fin
    prevPrev = 0
    prev = 1
    res = 0
    i = 2
    mientras( i <= n)
        res = prev + prevPrev
        prevPrev = prev
        prev = res
        i = i + 1
    fin
    retorno res
fin
res = "fibo_iter: "
res ..= fibo_iter(8)
poner(res)
